extends _layout
block meta
    meta(charset="UTF-8")
    meta(name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no")
    meta(http-equiv="X-UA-Compatible" content="IE=edge,chrome=1")
block styles
    link(rel="stylesheet" href="server/views/calendar-files/styles.css")
block scripts
    script(src="./calendar-files/date-format.js")
    script(src="./calendar-files/ui.js")
    script(src="./calendar-files/date-picker.js")
block container
        div.alert.alert-dismissible.alert-danger
            button.close(type="button" data-dismiss="alert") &times;
            strong You must be logged in to make new travel advertisement. 
            a.alert-link(href="#") Log in now.
